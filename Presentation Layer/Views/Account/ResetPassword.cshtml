@model ResendPasswordViewModel
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout1.cshtml";
}

<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All" class="text-denger"></div>
		<form asp-action="ResetPassword">

			<input type="password" asp-for="Password" placeholder="Enter the new Password" />
			<span asp-validation-for="Password" class="text-danger"></span>
			<br />
			<input type="password" asp-for="ConfirmPassword" placeholder="Enter the  ConfirmPassword" />
			<span asp-validation-for="ConfirmPassword" class="text-danger"></span>
			<br />

			<input type="submit" value="Rest Password">
		</form>

	</div>
</div>
@section ValidationScripts
{
	<partial name="_ValidationScriptsPartial" />

}

